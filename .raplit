# .replit - Replit configuration file
run = "python main.py"
language = "python3"
entrypoint = "main.py"
modules = ["python-3.11"]

[nix]
channel = "stable-22_11"

[env]
VIRTUAL_ENV = "/home/runner/.local/share/virtualenvs/python3"

[packager]
language = "python3"

[packager.features]
packageSearch = true
guessImports = true
enabledForHosting = false

[unitTest]
language = "python3"

[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = "pyls"

[[ports]]
localPort = 5000
externalPort = 80

[agent]
expertMode = true
